{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/main.mjs",
            "args": [
                "${workspaceFolder}/tmp/src",
                "${workspaceFolder}/tmp/dst",
                "-ddp", "{typePluralLowerCase}",
                "-dfp", "{name} ({no}).cal",
                "-de"
            ],
            "console": "integratedTerminal"
        }
    ]
}